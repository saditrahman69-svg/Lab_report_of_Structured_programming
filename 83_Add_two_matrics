#include <stdio.h>

int main() {
    int a[10][10], b[10][10], c[10][10], r, col;
    printf("Enter rows & columns: ");
    scanf("%d %d", &r, &col);

    printf("Enter first matrix:\n");
    for (int i = 0; i < r; i++)
        for (int j = 0; j < col; j++)
            scanf("%d", &a[i][j]);

    printf("Enter second matrix:\n");
    for (int i = 0; i < r; i++)
        for (int j = 0; j < col; j++)
            scanf("%d", &b[i][j]);

    for (int i = 0; i < r; i++)
        for (int j = 0; j < col; j++)
            c[i][j] = a[i][j] + b[i][j];

    printf("Sum of matrices:\n");
    for (int i = 0; i < r; i++) {
        for (int j = 0; j < col; j++)
            printf("%d ", c[i][j]);
        printf("\n");
    }
    return 0;
}
